// Test: Interfaces with static methods, methods, and fields

interface Complex {
    fn complex_method(self) -> i32;
    static fn complex_static_method();
    let complex_field: i32;
}

// A class implementing Complex
class Baz extend Complex {
    fn complex_method(self) -> i32 {
        return self.complex_field * 2;
    }

    static fn complex_static_method() {
        print("Baz static method");
    }

    let complex_field: i32 = 20;
}

fn main() -> i32 {
    // Test Complex interface
    let baz_instance = Baz{};
    print(baz_instance.complex_method()); // Should print 40
    Baz::complex_static_method(); // Should print "Baz static method"
    return 0;
}

