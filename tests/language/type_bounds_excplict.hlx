interface Addable {
    op + fn (self, other: self) -> self;
}

fn add(a: T, b: T) -> T requires <T> if Addable in T {
    return a + b;
}

fn main() -> i32 {
    let result = add::<i32>(10, 20);
    print(result);
    return 0;
}

