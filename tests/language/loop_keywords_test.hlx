import std::range::*;

fn main() -> i32 {
    // Test: Simple for loop
    for i in 0..5 {
        print(i); // Prints 0 through 4
    }

    // Test: while loop with break
    let x = 0;
    while x < 5 {
        if x == 3 {
            break;
        }
        print(x);
        x = x + 1;
    }

    // Test: continue in loop
    for i in 0..=5 {
        if i == 2 {
            continue;
        }
        print(i); // Skips 2
    }

    return 0;
}

