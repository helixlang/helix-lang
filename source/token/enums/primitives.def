#define PRIMITIVE_TOKENS(GENERATE)         \
    GENERATE(PRIMITIVE_VOID,    "void"   ) \
    GENERATE(PRIMITIVE_BOOL,    "bool"   ) \
    GENERATE(PRIMITIVE_BYTE,    "byte"   ) \
    GENERATE(PRIMITIVE_CHAR,    "char"   ) \
    GENERATE(PRIMITIVE_POINTER, "ptr"    ) \
    GENERATE(PRIMITIVE_I8,      "i8"     ) \
    GENERATE(PRIMITIVE_U8,      "u8"     ) \
    GENERATE(PRIMITIVE_I16,     "i16"    ) \
    GENERATE(PRIMITIVE_U16,     "u16"    ) \
    GENERATE(PRIMITIVE_I32,     "i32"    ) \
    GENERATE(PRIMITIVE_U32,     "u32"    ) \
    GENERATE(PRIMITIVE_F32,     "f32"    ) \
